		<PackageReleaseNotes>
			# Early release
		</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
		<None Include="icon.png" Pack="true" PackagePath="\" />
		<None Include="icon.svg" Pack="false" />
	</ItemGroup>

</Project>

